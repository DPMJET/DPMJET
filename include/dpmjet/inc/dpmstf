      LOGICAL LLLNUC
      INTEGER IBDUMM, ICDUMM, IDUMMY
      DOUBLE PRECISION AMNDUM, AMELCT, ABDUMM, ZCDUMM, WDUMMY
      DOUBLE PRECISION AMNAMA, EXMSAZ, PFRMAV, CLFLEV
      PARAMETER ( AMELCT = 0.5109989461D-03 )
*  Statement functions
      AMNAMA (AMNDUM,IBDUMM,ICDUMM) = AMNDUM+DBLE(ICDUMM)*AMELCT
      EXMSAZ (ABDUMM,ZCDUMM,LLLNUC,IDUMMY) = 0.D+00
      PFRMAV (IDUMMY) = 0.22D+00
      CLFLEV (WDUMMY) = WDUMMY
